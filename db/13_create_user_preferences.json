{
  "databaseChangeLog": [
    {
      "changeSet": {
        "id": "12",
        "author": "zach",
        "changes": [
          {
            "createTable": {
              "tableName": "user_preferences",
              "columns": [
                {
                  "column": {
                    "name": "id",
                    "type": "BIGINT",
                    "constraints": {
                      "primaryKey": true,
                      "nullable": false,
                      "primaryKeyName": "pk_user_preferences"
                    }
                  }
                },
                {
                  "column": {
                    "name": "user_id",
                    "type": "BIGINT",
                    "constraints": {
                      "nullable": false,
                      "foreignKeyName": "user_preferences_user_id_fk",
                      "references": "user(id)"
                    }
                  }
                },
                {
                  "column": {
                    "name": "time_zone",
                    "type": "text",
                    "constraints": {
                      "nullable": false
                    },
                    "defaultValue": "America/New_York"
                  }
                }
              ]
            }
          }
        ]
      }
    }
  ]
}
